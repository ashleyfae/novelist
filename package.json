{
  "name": "novelist",
  "version": "1.1.11",
  "description": "Easily organize and display your portfolio of books.",
  "author": "Nose Graze",
  "license": "GPL-2.0-or-later",
  "homepage": "https://novelistplugin.com",
  "devDependencies": {
    "@wordpress/components": "^8.1.0",
    "@wordpress/i18n": "^3.17.0",
    "@wordpress/scripts": "^16.0.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "wp-scripts build --webpack-no-externals && npm run i18n",
    "format": "wp-scripts format",
    "i18n": "wp i18n make-pot . languages/novelist.pot --domain=novelist",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "dev": "wp-scripts start --webpack-no-externals"
  }
}
